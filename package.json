    "eslint-plugin-o1js": "^0.4.0",
    "jest": "^28.1.3",
    "prettier": "^2.3.2",
    "ts-jest": "^28.0.8",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "o1js": "^1.2.0"
